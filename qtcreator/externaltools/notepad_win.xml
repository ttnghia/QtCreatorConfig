<externaltool id="notepad">
    <description>Opens the current file in Notepad++</description>
    <displayname>Edit with Notepad</displayname>
    <category>Text</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="no">
        <path>C:/Program Files (x86)/Notepad++/notepad++.exe</path>
        <arguments>&quot;%{CurrentDocument:FilePath}&quot;</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
